#container
	= render 'layouts/sidebar-nav'
	#content
		.content-heading 
			%h2 Listings
		#listings
			#listing
				#home-img
					%img{:src => image_path("home-img.jpg")}
				#listing-info
					%h2#li Irvine California 
					%h5#li 12842 King Canyon Rd.
					%h5#li Single Family Home
					%h5#li Price: 1,200,000
					#li#sms SMS: Text 96 to (714) 317-9653
				.right-details
					.qr-container
						#qr-code
							%img{:src => image_path("qr.png")}
						#qr-download
							%img{:src => image_path("qr-download.png")} 
							#download Download
					%ul.options
						%li#first
							#view 
								%a{:src => 'http://www.google.com'}
						%li
							#edit 
								%a					
						%li
							#delete 
								%a
				%hr				
				

